{
    "name": "pmmotors/google-apiclient",
    "description": "Google api php client wrapper with Cloud Platform and Laravel 4 & 5 support",
    "homepage": "https://github.com/pmmotors/google-apiclient",
    "keywords": ["laravel", "google", "cloud platform"],
    "license": "MIT",
    "authors": [
        {
            "name": "Pixel Motion"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "~5",
        "google/apiclient": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
        "psr-4": {
            "PmMotors\\Google\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        },
        "laravel": {
            "providers": [
                "PmMotors\\Google\\GoogleServiceProvider"
            ],
            "aliases": {
                "Google": "PmMotors\\Google\\Facades\\Google"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}